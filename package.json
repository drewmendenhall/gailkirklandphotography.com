{
  "name": "gailkirklandphotography-www",
  "version": "1.0.0",
  "description": "The source that powers the photo portfolio web site for [Gail Kirkland Photography](gailkirklandphotography.com)",
  "repository": {
    "type": "git",
    "url": "https://code.mendenhall.io/drew/gailkirklandphotography-www"
  },
  "bugs": "https://code.mendenhall.io/drew/gailkirklandphotography-www/issues",
  "license": "UNLICENSED",
  "author": "Drew Mendenhall <drew@mendenhall.io>",
  "scripts": {
    "build:dev": "mkdirp public && npm run build:styles:dev",
    "build:prod": "mkdirp dist public && ((npm run build:styles:prod & npm run build:scripts:prod & npm run build:server:prod) && cp -r public dist)",
    "build:server:prod": "webpack --config webpack.config.server.production.babel.js",
    "build:scripts:prod": "webpack --config webpack.config.production.babel.js",
    "build:styles": "lessc styles/index.less public/app.css --npm-import --strict-math=on",
    "build:styles:dev": "npm run build:styles -- --source-map",
    "build:styles:prod": "npm run build:styles -- --autoprefix --clean-css",
    "clean:all": "npm run clean:scripts & npm run clean:styles",
    "clean:npm": "rimraf node_modules",
    "clean:scripts": "rimraf public/app.js",
    "clean:styles": "rimraf public/app.css*",
    "serve:debug": "npm run serve:dev --debug & node-inspector",
    "serve:dev": "nodemon --exec babel-node --plugins transform-es2015-modules-commonjs server",
    "serve:prod": "npm run serve:prod:www & npm run serve:prod:monitor",
    "serve:prod:www": "forever start --append --uid 'gailkirklandphotography-www' dist/server.js",
    "serve:prod:monitor": "forever start --append --uid 'gailkirklandphotography-www-monitor' dist/monitor.js",
    "serve:prod:restart": "forever restart gailkirklandphotography-www",
    "serve:prod:stop": "forever stop gailkirklandphotography-www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run watch:styles",
    "watch:styles": "grunt watch:css & onchange 'styles/**/*' -- npm run build:styles:dev",
    "dev": "npm run build:dev && (npm run watch & npm run serve:dev)",
    "prod": "npm run build:prod && npm run serve:prod",
    "start": "npm run prod"
  },
  "dependencies": {
    "analytics-node": "^2.0.2",
    "babel-runtime": "^6.6.1",
    "body-parser": "^1.15.0",
    "classnames": "^2.2.3",
    "connect-slashes": "^1.3.1",
    "express": "^4.13.4",
    "forever": "^0.15.1",
    "normalize.css": "^4.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-helmet": "^3.0.0",
    "react-router": "^2.0.1",
    "serve-static": "^1.10.2"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-inline-environment-variables": "^6.5.0",
    "babel-plugin-transform-node-env-inline": "^6.5.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.7.2",
    "connect-livereload": "^0.5.4",
    "eslint": "^2.5.3",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-watch": "^1.0.0",
    "json-loader": "^0.5.4",
    "less": "^2.6.1",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-npm-import": "^2.1.0",
    "load-grunt-config": "^0.19.1",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.9.1",
    "onchange": "^2.1.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.2",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-node-externals": "^1.0.0"
  }
}
