{
  "name": "gailkirklandphotography-www",
  "version": "0.0.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://code.mendenhall.io/drew/gailkirklandphotography-www"
  },
  "scripts": {
    "build": "npm run build:styles & npm run build:scripts",
    "build:scripts": "webpack --progress",
    "build:styles": "mkdirp public/styles && cp node_modules/normalize.css/normalize.css public/styles/ & npm run build:styles:app:dev",
    "build:styles:app": "lessc styles/index.less public/styles/app.css",
    "build:styles:app:dev": "npm run build:styles:app -- --source-map",
    "clean:all": "npm run clean:scripts & npm run clean:styles",
    "clean:scripts": "rimraf public/bundle.js",
    "clean:styles": "rimraf public/styles",
    "serve": "nodemon server.js",
    "start": "npm run build:styles && (npm run watch & npm run serve)",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run watch:styles",
    "watch:styles": "grunt watch:css & onchange 'styles/**/*' -- npm run build:styles:app:dev"
  },
  "author": "Drew Mendenhall",
  "dependencies": {
    "babel": "5",
    "babel-runtime": "5",
    "bootstrap": "3",
    "classnames": "2",
    "connect": "3",
    "connect-slashes": "1",
    "lodash.merge": "3",
    "normalize.css": "3",
    "object-assign": "4",
    "react": "0.13",
    "react-router": "0.13",
    "serve-static": "1"
  },
  "devDependencies": {
    "babel-core": "5",
    "babel-eslint": "4",
    "babel-loader": "5",
    "connect-livereload": "0.5",
    "eslint": "1",
    "eslint-plugin-react": "3",
    "grunt": "0.4",
    "grunt-cli": "0.1",
    "grunt-contrib-watch": "0.6",
    "less": "2",
    "load-grunt-config": "0.17",
    "mkdirp": "0.5",
    "nodemon": "1",
    "onchange": "2",
    "react-hot-loader": "1",
    "rimraf": "2",
    "webpack": "1",
    "webpack-dev-server": "1"
  }
}
