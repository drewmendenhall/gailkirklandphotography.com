{
  "name": "gailkirklandphotography-www",
  "version": "3.0.10",
  "description": "The source that powers the photo portfolio web site for [Gail Kirkland Photography](gailkirklandphotography.com)",
  "repository": {
    "type": "git",
    "url": "https://code.mendenhall.io/drew/gailkirklandphotography-www"
  },
  "bugs": "https://code.mendenhall.io/drew/gailkirklandphotography-www/issues",
  "license": "UNLICENSED",
  "author": "Drew Mendenhall <drew@mendenhall.io>",
  "scripts": {
    "build": "run-p build:*",
    "build:client": "gulp client",
    "build:images": "gulp images",
    "build:server": "gulp server && ln -s -fn ../public dist/public",
    "build:styles": "gulp styles",
    "clean": "run-p clean:*",
    "clean:client": "rimraf public/app.js public/vendor.js",
    "clean:images": "rimraf public/images",
    "clean:server": "rimraf dist",
    "clean:styles": "rimraf public/app.css*",
    "serve:dev": "babel-watch --exclude components server",
    "serve:debug": "babel-node --debug -- server/index.js --debug & node-inspector",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postversion": "git push && git push --tags",
    "watch": "run-p watch:*",
    "watch:styles": "gulp watch:styles",
    "dev": "run-p build:images build:styles serve:dev watch",
    "start": "node dist/server"
  },
  "dependencies": {
    "ajv": "^6.1.1",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.5",
    "compression": "^1.7.2",
    "connect-slashes": "^1.3.1",
    "dotenv-safe": "^5.0.1",
    "express": "^4.16.2",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-helmet": "^5.2.0",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "serve-static": "^1.13.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-dev": "^1.0.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-node-env-inline": "^0.3.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "bluebird": "^3.5.1",
    "connect-livereload": "^0.6.0",
    "eslint": "^4.18.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.7.0",
    "gulp": "^3.9.1",
    "gulp-filter": "^5.1.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-less": "^4.0.0",
    "gulp-livereload": "^3.8.1",
    "gulp-newer": "^1.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.4",
    "imagemagick": "^0.1.3",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-npm-import": "^2.1.0",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-node-externals": "^1.2.0"
  }
}
