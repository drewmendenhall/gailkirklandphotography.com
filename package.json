{
  "name": "gailkirklandphotography-www",
  "version": "0.0.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://code.mendenhall.io/drew/gailkirklandphotography-www"
  },
  "scripts": {
    "build:dev": "npm run build:styles:dev",
    "build:prod": "npm run build:styles:prod & npm run build:scripts:prod",
    "build:scripts": "webpack --progress",
    "build:scripts:prod": "npm run build:scripts -- --config webpack.config.production",
    "build:styles": "mkdirp public/styles && lessc styles/index.less public/styles/app.css --npm-import",
    "build:styles:dev": "npm run build:styles -- --source-map",
    "build:styles:prod": "npm run build:styles -- --autoprefix --clean-css",
    "clean:all": "npm run clean:scripts & npm run clean:styles",
    "clean:scripts": "rimraf public/bundle.js",
    "clean:styles": "rimraf public/styles",
    "serve": "nodemon server.js",
    "serve:debug": "npm run serve --debug & node-inspector",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run watch:styles",
    "watch:styles": "grunt watch:css & onchange 'styles/**/*' -- npm run build:styles:dev",
    "dev": "npm run build:dev && (npm run watch & npm run serve)",
    "prod": "npm run build:prod && npm run serve",
    "start": "npm run prod"
  },
  "author": "Drew Mendenhall",
  "dependencies": {
    "analytics-node": "^1.2.2",
    "babel": "5",
    "babel-runtime": "5",
    "bootstrap": "3",
    "classnames": "2",
    "connect": "3",
    "connect-slashes": "1",
    "lodash.merge": "3",
    "normalize.css": "3",
    "object-assign": "4",
    "path-to-regexp": "1",
    "react": "0.13",
    "react-router": "0.13",
    "serve-static": "1"
  },
  "devDependencies": {
    "babel-core": "5",
    "babel-eslint": "4",
    "babel-loader": "5",
    "connect-livereload": "0.5",
    "eslint": "1",
    "eslint-plugin-react": "3",
    "grunt": "0.4",
    "grunt-cli": "0.1",
    "grunt-contrib-watch": "0.6",
    "less": "2",
    "less-plugin-autoprefix": "1",
    "less-plugin-clean-css": "1",
    "less-plugin-npm-import": "2",
    "load-grunt-config": "0.17",
    "mkdirp": "0.5",
    "nodemon": "1",
    "onchange": "2",
    "react-hot-loader": "1",
    "rimraf": "2",
    "webpack": "1",
    "webpack-dev-server": "1"
  }
}
