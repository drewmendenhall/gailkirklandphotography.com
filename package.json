{
  "name": "gailkirklandphotography-www",
  "version": "1.1.4",
  "description": "The source that powers the photo portfolio web site for [Gail Kirkland Photography](gailkirklandphotography.com)",
  "repository": {
    "type": "git",
    "url": "https://code.mendenhall.io/drew/gailkirklandphotography-www"
  },
  "bugs": "https://code.mendenhall.io/drew/gailkirklandphotography-www/issues",
  "license": "UNLICENSED",
  "author": "Drew Mendenhall <drew@mendenhall.io>",
  "scripts": {
    "build": "mkdirp dist public && npm-run-all --parallel build:* && rsync --archive --delete public dist",
    "build:client": "gulp client",
    "build:server": "gulp server",
    "build:styles": "gulp styles",
    "clean": "npm-run-all --parallel clean:*",
    "clean:npm": "rimraf node_modules",
    "clean:client": "rimraf public/app.js public/vendor.js",
    "clean:prod": "rimraf dist",
    "clean:server": "rimraf dist/monitor.js dist/server.js",
    "clean:styles": "rimraf public/app.css*",
    "serve:debug": "npm run serve:dev --debug & node-inspector",
    "serve:dev": "nodemon --exec babel-node --presets es2015-node server",
    "serve:prod": "npm run serve:prod:www & npm run serve:prod:monitor",
    "serve:prod:www": "forever start --append --uid 'gailkirklandphotography-www' dist/server.js",
    "serve:prod:monitor": "forever start --append --uid 'gailkirklandphotography-www-monitor' dist/monitor.js",
    "serve:prod:restart": "forever restart gailkirklandphotography-www",
    "serve:prod:stop": "forever stop gailkirklandphotography-www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm-run-all --parallel watch:*",
    "watch:styles": "gulp watch:styles",
    "dev": "npm run build:styles && npm-run-all --parallel watch serve:dev",
    "prod": "npm run build && npm run serve:prod",
    "start": "npm run prod"
  },
  "dependencies": {
    "analytics-node": "^2.0.2",
    "babel-runtime": "^6.6.1",
    "body-parser": "^1.15.0",
    "classnames": "^2.2.3",
    "connect-slashes": "^1.3.1",
    "express": "^4.13.4",
    "forever": "^0.15.1",
    "normalize.css": "^4.0.0",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-helmet": "^3.0.0",
    "react-router": "^2.0.1",
    "serve-static": "^1.10.2"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-inline-environment-variables": "^6.5.0",
    "babel-plugin-transform-node-env-inline": "^6.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.6.5",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node": "^5.0.3",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.7.2",
    "connect-livereload": "^0.5.4",
    "eslint": "^2.5.3",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "gulp": "^3.9.1",
    "gulp-filter": "^4.0.0",
    "gulp-less": "^3.0.5",
    "gulp-livereload": "^3.8.1",
    "gulp-newer": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "json-loader": "^0.5.4",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-npm-import": "^2.1.0",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.9.1",
    "npm-run-all": "^1.7.0",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-node-externals": "^1.0.0"
  }
}
