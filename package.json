{
  "name": "gailkirklandphotography-www",
  "version": "0.0.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://code.mendenhall.io/drew/gailkirklandphotography-www"
  },
  "scripts": {
    "build:dev": "mkdirp public && npm run build:styles:dev",
    "build:prod": "mkdirp public && npm run build:styles:prod & npm run build:scripts:prod",
    "build:scripts": "webpack --progress",
    "build:scripts:prod": "npm run build:scripts -- --config webpack.config.production",
    "build:styles": "lessc styles/index.less public/app.css --npm-import --strict-math=on",
    "build:styles:dev": "npm run build:styles -- --source-map",
    "build:styles:prod": "npm run build:styles -- --autoprefix --clean-css",
    "clean:all": "npm run clean:scripts & npm run clean:styles",
    "clean:npm": "rimraf node_modules",
    "clean:scripts": "rimraf public/app.js",
    "clean:styles": "rimraf public/app.css*",
    "serve:debug": "npm run serve:dev --debug & node-inspector",
    "serve:dev": "nodemon --exec babel-node server.js",
    "serve:prod": "forever start -f --uid 'gailkirklandphotography-www' babel-server.js",
    "serve:prod:restart": "forever restart gailkirklandphotography-www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "npm run watch:styles",
    "watch:styles": "grunt watch:css & onchange 'styles/**/*' -- npm run build:styles:dev",
    "dev": "npm run build:dev && (npm run watch & npm run serve:dev)",
    "prod": "npm run build:prod && npm run serve:prod",
    "start": "npm run prod"
  },
  "author": "Drew Mendenhall",
  "dependencies": {
    "analytics-node": "2",
    "babel": "5",
    "babel-runtime": "5",
    "classnames": "2",
    "connect-slashes": "1",
    "express": "4",
    "forever": "0.15",
    "history": "1",
    "newrelic": "^1.22.1",
    "normalize.css": "3",
    "react": "0.14",
    "react-dom": "0.14",
    "react-helmet": "2",
    "react-router": "1",
    "serve-static": "1"
  },
  "devDependencies": {
    "babel-eslint": "5.0.0-beta4",
    "babel-loader": "5",
    "babel-plugin-react-transform": "1",
    "connect-livereload": "0.5",
    "eslint": "1",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "3",
    "grunt": "0.4",
    "grunt-cli": "0.1",
    "grunt-contrib-watch": "0.6",
    "json-loader": "^0.5.3",
    "less": "2",
    "less-plugin-autoprefix": "1",
    "less-plugin-clean-css": "1",
    "less-plugin-npm-import": "2",
    "load-grunt-config": "0.19",
    "mkdirp": "0.5",
    "nodemon": "1",
    "onchange": "2",
    "react-transform-catch-errors": "1",
    "react-transform-hmr": "1",
    "redbox-react": "1",
    "rimraf": "2",
    "webpack": "1",
    "webpack-dev-middleware": "1",
    "webpack-hot-middleware": "2"
  }
}
