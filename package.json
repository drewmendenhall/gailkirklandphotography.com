{
  "name": "gailkirklandphotography-www",
  "version": "3.0.10",
  "description": "The source that powers the photo portfolio web site for [Gail Kirkland Photography](gailkirklandphotography.com)",
  "repository": {
    "type": "git",
    "url": "https://code.mendenhall.io/drew/gailkirklandphotography-www"
  },
  "bugs": "https://code.mendenhall.io/drew/gailkirklandphotography-www/issues",
  "license": "UNLICENSED",
  "author": "Drew Mendenhall <drew@mendenhall.io>",
  "scripts": {
    "build": "run-p build:*",
    "build:client": "gulp client",
    "build:images": "gulp images",
    "build:server": "gulp server && ln -s -fn ../public dist/public",
    "build:styles": "gulp styles",
    "clean": "run-p clean:*",
    "clean:client": "rimraf public/*app.js",
    "clean:images": "rimraf public/images",
    "clean:server": "rimraf dist",
    "clean:styles": "rimraf public/app.css*",
    "format": "prettier-eslint --write '**/*.{js{,on},y{,a}ml}'",
    "serve:dev": "babel-watch --exclude components server",
    "serve:debug": "babel-node --debug -- server/index.js --debug & node-inspector",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postversion": "git push && git push --tags",
    "watch": "run-p watch:*",
    "watch:styles": "gulp watch:styles",
    "dev": "run-p build:images build:styles serve:dev watch",
    "start": "node dist/server"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "connect-slashes": "^1.4.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "gulp-util": "^3.0.8",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-react-constant-elements": "^7.6.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.0",
    "@hot-loader/react-dom": "^16.9.0",
    "babel-loader": "^8.0.6",
    "babel-watch": "^7.0.0",
    "connect-livereload": "^0.6.1",
    "eslint": "^5.16.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^2.0.1",
    "gulp": "^4.0.2",
    "gulp-filter": "^6.0.0",
    "gulp-imagemin": "^6.0.0",
    "gulp-less": "^4.0.1",
    "gulp-livereload": "^4.0.1",
    "gulp-newer": "^1.4.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "imagemagick": "^0.1.3",
    "less-plugin-autoprefix": "^2.0.0",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-npm-import": "^2.1.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier-eslint-cli": "^5.0.0",
    "react-hot-loader": "^4.12.13",
    "rimraf": "^3.0.0",
    "webpack": "^4.40.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">=10"
  }
}
