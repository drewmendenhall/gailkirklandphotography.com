{
  "name": "gailkirklandphotography-www",
  "version": "1.2.1",
  "description": "The source that powers the photo portfolio web site for [Gail Kirkland Photography](gailkirklandphotography.com)",
  "repository": {
    "type": "git",
    "url": "https://code.mendenhall.io/drew/gailkirklandphotography-www"
  },
  "bugs": "https://code.mendenhall.io/drew/gailkirklandphotography-www/issues",
  "license": "UNLICENSED",
  "author": "Drew Mendenhall <drew@mendenhall.io>",
  "scripts": {
    "build": "run-p build:* && rsync --archive --delete public dist",
    "build:client": "gulp client",
    "build:images": "gulp images",
    "build:server": "gulp server",
    "build:styles": "gulp styles",
    "clean": "run-p clean:*",
    "clean:client": "rimraf public/app.js public/vendor.js",
    "clean:images": "rimraf public/images",
    "clean:prod": "rimraf dist",
    "clean:server": "rimraf dist/monitor.js dist/server.js",
    "clean:styles": "rimraf public/app.css*",
    "serve:dev": "babel-watch --exclude components server",
    "serve:debug": "babel-node --debug -- server/index.js --debug & node-inspector",
    "serve:prod": "run-p serve:prod:*",
    "serve:prod:www": "forever start --append --uid 'gailkirklandphotography-www' dist/server.js",
    "serve:prod:monitor": "forever start --append --uid 'gailkirklandphotography-www-monitor' dist/monitor.js",
    "serve:prod:www:restart": "forever restart gailkirklandphotography-www",
    "serve:prod:www:stop": "forever stop gailkirklandphotography-www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postversion": "git push && git push --tags",
    "watch": "run-p watch:*",
    "watch:styles": "gulp watch:styles",
    "dev": "run-p build:images build:styles && run-p watch serve:dev",
    "prod": "npm run build && npm run serve:prod",
    "start": "npm run prod"
  },
  "dependencies": {
    "analytics-node": "^2.1.0",
    "babel-runtime": "^6.9.0",
    "chokidar": "^1.5.1",
    "classnames": "^2.2.5",
    "compression": "^1.6.1",
    "connect-slashes": "^1.3.1",
    "dotenv-safe": "^2.3.1",
    "express": "^4.13.4",
    "forever": "^0.15.2",
    "npm-run-all": "^2.1.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-helmet": "^3.1.0",
    "react-router": "^2.4.1",
    "serve-static": "^1.10.2",
    "time-stamp": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.8.0",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.8.0",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.9.0",
    "babel-watch": "^2.0.2",
    "bluebird": "^3.4.0",
    "connect-livereload": "^0.5.4",
    "eslint": "^2.11.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.1.1",
    "gulp": "^3.9.1",
    "gulp-filter": "^4.0.0",
    "gulp-imagemin": "^3.0.1",
    "gulp-less": "^3.1.0",
    "gulp-livereload": "^3.8.1",
    "gulp-newer": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "imagemagick": "^0.1.3",
    "json-loader": "^0.5.4",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-npm-import": "^2.1.0",
    "normalize.css": "^4.1.1",
    "rimraf": "^2.5.2",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-node-externals": "^1.0.0"
  }
}
