{
  "blacklist": ["minification.deadCodeElimination"],
  "env": {
    "development": {
      "plugins": ["react-transform"],
      "extra": {
        "react-transform": {
          "transforms": [{
            "transform": "react-transform-catch-errors",
            "imports": ["react", "redbox-react"]
          }]
        }
      },
      "optional": ["validation"]
    },
    "production": {
      "optional": [
        "minification",
        "optimisation"
      ]
    }
  },
  "optional": [
    "runtime",
    "utility.inlineEnvironmentVariables"
  ],
  "stage": 0
}
