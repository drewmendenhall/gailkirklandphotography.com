{
  "env": {
    "development": {
      "plugins": [
        ["transform-define", {
          "PROD": "false"
        }]
      ]
    },
    "production": {
      "plugins": [
        ["transform-define", {
          "PROD": "true"
        }]
      ],
      "presets": [
        "es2015"
      ]
    }
  },
  "plugins": [
    "transform-es2015-modules-commonjs",
    "transform-object-rest-spread",
    "transform-runtime"
  ]
}
