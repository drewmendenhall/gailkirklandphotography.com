{
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-catch-errors",
            "imports": ["react", "redbox-react"]
          }]
        }]
      ]
    },
    "production": {
      "plugins": [
        "transform-inline-environment-variables",
        "transform-node-env-inline",
        "transform-react-constant-elements",
        "transform-react-inline-elements"
      ]
    }
  },
  "presets": [
    "es2015",
    "react",
    "stage-1"
  ],
  "plugins": [
    "transform-runtime"
  ]
}
